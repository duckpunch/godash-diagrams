{
  "name": "godash-diagrams",
  "version": "0.8.0",
  "description": "A diagramming library for Go, inspired by Mermaid, and built on Godash",
  "author": {
    "name": "duckpunch",
    "url": "https://github.com/duckpunch"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/duckpunch/godash-diagrams.git"
  },
  "homepage": "https://duckpunch.github.io/godash-diagrams/",
  "keywords": [
    "go",
    "baduk",
    "weiqi",
    "diagram",
    "mermaid",
    "godash"
  ],
  "type": "module",
  "main": "./dist/godash-diagrams.umd.cjs",
  "module": "./dist/godash-diagrams.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/godash-diagrams.js",
      "require": "./dist/godash-diagrams.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build && vite build --config vite.config.auto.ts",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "godash": "^2.5.2"
  }
}
